import sys
import math
def gcd(a,b):
    while b:
        a,b=b,a%b
    return a
def main():
    n=int(sys.stdin.readline().strip())
    P=list(map(int,sys.stdin.readline().strip().split()))
    g=P[0]
    for i in range(1,n):
        g=gcd(g,P[i])
    print(g*n)
if __name__=="__main__":
    main()
